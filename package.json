{
  "name": "slashbinteam-website",
  "private": true,
  "description": "The Official Website for the SlashBin Team",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "deploy": "gatsby build && gh-pages -d public -b master",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "gatsby": "^2.24.54",
    "gatsby-plugin-postcss": "^2.3.11",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gh-pages": "^3.1.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "prettier": "2.1.1",
    "tailwindcss": "^1.8.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SlashBinTeam/slashbinteam.github.io"
  },
  "bugs": {
    "url": "https://github.com/SlashBinTeam/slashbinteam.github.io/issues"
  }
